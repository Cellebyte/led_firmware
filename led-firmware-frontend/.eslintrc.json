{
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "@vue/airbnb",
      "@vue/typescript/recommended"
    ],
    "parser": "vue-eslint-parser",
    "plugins": [
        "@typescript-eslint",
        "vue"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "parser": "@typescript-eslint/parser",
      "sourceType": "module"
    },
    "settings": {
        "import/extensions": [
            ".ts",
            ".tsx",
            ".vue"
        ],
        "import/resolver": {
            "node": {
              "extensions": [
                ".ts",
                ".tsx",
                ".vue"
              ]
            }
          }
    },
    "rules": {}
}